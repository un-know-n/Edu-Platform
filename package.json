    "@nestjs/core": "^10.0.2",
    "@nestjs/mapped-types": "^2.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "@prisma/client": "^5.4.2",
    "@reduxjs/toolkit": "^1.9.6",
    "axios": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "formik": "^2.4.5",
    "framer-motion": "^10.16.4",
    "next": "13.4.1",
    "next-auth": "^4.23.1",
    "next-redux-wrapper": "^8.1.0",
    "pg": "^8.11.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.11.0",
    "react-redux": "^8.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.22.2"
  },
