{
  "name": "@edu-platform/source",
  "version": "0.0.0",
  "license": "MIT",
  "prisma": {
    "schema": "apps/server/prisma/schema.prisma"
  },
  "scripts": {
    "server:dev": "npx nx serve server",
    "client:dev": "npx nx serve client",
    "start:dev": "npx nx run-many --target=serve --projects=client,server",
    "client:production": "npx nx build client && npx nx serve client --prod",
    "server:production": "npx nx build server && npx nx serve server --prod",
    "build:production": "npx nx run-many --target=build --projects=client,server --prod",
    "start:production": "npx nx run-many --target=serve --projects=client,server --prod",
    "migrate:dev": "npx prisma migrate dev",
    "husky:prepare": "npx husky install",
    "commit:prompt": "git-cz",
    "setup:all": "npm i && npm run husky:prepare && migrate:dev"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "private": true,
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.3",
    "@chakra-ui/next-js": "^2.1.5",
    "@chakra-ui/react": "^2.8.1",
