    "@nx/linter": "16.9.1",
    "@nx/nest": "16.9.1",
    "@nx/next": "^16.9.1",
    "@nx/node": "16.9.1",
    "@nx/react": "16.9.1",
    "@nx/webpack": "16.9.1",
    "@nx/workspace": "16.9.1",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.4.1",
